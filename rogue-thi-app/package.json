{
  "name": "rogue-thi-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "npm run prebuild",
    "dev": "next dev",
    "prebuild": "npm run compile-scss",
    "compile-scss": "sass --no-source-map --style compressed scss/themes/:public/themes/",
    "build": "next build",
    "start": "next start",
    "android-add": "rm -rf android/ && npx cap add android",
    "android-open": "npm run prebuild && next build && next export && npx cap sync android && npx cap open android",
    "android-run": "npm run prebuild && next build && next export && npx cap sync android && npx cap run android",
    "ios-open": "npm run prebuild && next build && next export && npx cap sync ios && npx cap open ios",
    "ios-run": "npm run prebuild && next build && next export && npx cap sync ios && npx cap run ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@capacitor-community/http": "^1.4.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@restart/hooks": "^0.4.7",
    "bootstrap": "^4.6.1",
    "cheerio": "^1.0.0-rc.10",
    "deepl-node": "^1.10.2",
    "dompurify": "^2.3.4",
    "fetch-bypass-cors": "github:neuland-ingolstadt/fetch-bypass-cors#v1.0.3",
    "fetch-cookie": "^2.0.1",
    "i18next": "^22.5.0",
    "ical-generator": "^3.6.1",
    "idb": "^7.0.0",
    "leaflet": "^1.7.1",
    "next": "^12.1.0",
    "next-i18next": "^13.2.2",
    "node-fetch": "^3.2.3",
    "pdf-parse": "^1.1.1",
    "postgres-array": "^3.0.2",
    "prop-types": "^15.8.0",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-dom": "^17.0.2",
    "react-i18next": "^12.3.1",
    "react-leaflet": "^3.2.2",
    "react-placeholder": "^4.1.0",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "sass": "^1.45.1",
    "win95.css": "github:neuland-ingolstadt/win95.css#commit=477a22394b61b57b20ee2c3a83b769c9a6dc8e46",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@capacitor/android": "^4.6.2",
    "@capacitor/cli": "^4.6.2",
    "@capacitor/core": "^4.6.2",
    "@capacitor/ios": "^4.6.2",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.3"
  }
}
